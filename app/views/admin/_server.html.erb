<section>
  <%=  form_for server, as: :post, url: edit_server_path(server.id) do  |f| %>
      <%=  f.label :name %>
      <%= f.text_field :name %>
      <%=  f.label :protocol %>
      <%= f.text_field :protocol %>
      <%=  f.label :server_address %>
      <%= f.text_field :server_address %>
      <%=  f.label :server_user %>
      <%= f.text_field :server_user %>
      <%=  f.label :password_digest %>
      <%= f.text_field :password_digest %>
      <%=  f.label :local_dir %>
      <%= f.text_field :local_dir %>
      <%=  f.label :edit_with_branches %>
      <%= f.text_field :edit_with_branches %>
      <%= f.submit 'Edit' %>
  <% end %>
</section>
